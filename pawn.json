{
    "user": "Central-Universe",
    "repo": "Timer-Fix-plugin",
    "resources": [
        {
            "name": "^timerfix-(.+)-win32.zip$",
            "platform": "windows",
            "archive": true,
            "includes": ["include"],
            "plugins": ["plugins/timerfix.dll"]
        },
        {
            "name": "^timerfix-(.+)-debian10.tar.gz$",
            "platform": "linux",
            "archive": true,
            "includes": ["include"],
            "plugins": ["plugins/timerfix.so"]
        }
    ],
    "runtime": {
        "plugins": ["Central-Universe/Timer-Fix-plugin"]
    }
}
